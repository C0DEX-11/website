{
  "name": "savetheinternet",
  "version": "1.0.0",
  "description": "Save the internet",
  "main": "./src/app.ts",
  "scripts": {
    "postsetup": "sudo npm install -g typescript",
    "build": "tsc",
    "watch": "npm run build -- -w",
    "start": "cross-env NODE_ENV=production node ./dist/app.js",
    "dev": "npm run build && node ./dist/app.js",
