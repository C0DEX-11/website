  "devDependencies": {
    "@types/node": "^10.3.4",
    "@types/winston": "^2.3.9",
    "autoprefixer": "^8.6.3",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.7.0",
    "mini-css-extract-plugin": "^0.4.0",
